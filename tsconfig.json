{
  "compilerOptions": {
    "target": "es5",
    "jsx": "react-jsx",
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "esModuleInterop": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "es6",
    "allowJs": true,
    "moduleResolution": "node"
  },
  "exclude": ["node_modules"]
}
